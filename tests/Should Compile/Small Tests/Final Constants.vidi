final Test {
  final A ::= "The A"
  final B ::= "Is "+A
  
  shared Y:Text:='d'
  
  Square(X:Integer):Integer { X*X }
  
  SomeText:Text { 'abc'+Y }
  
  final C ::= Square(5+2)
  final D ::= SomeText
}

// Test2 is Test {} // ERROR Test is final

{
  Console.PutLine(Test.A) // "Is The A"

  T:Test
  Console.PutLine(T.B) // "Is The A"

//  t.A:="aa"  // ERROR A is final

  Console.PutLine(Test.B)
  Console.PutLine(Test.C)
  Console.PutLine(Test.D)
}

