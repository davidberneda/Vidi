  // Generic Ancestor
  TW(V:Type) is Array(V) {}

  W:TW(Text)

  W.Append('W')

  Console.PutLine(W)

  // Generic subclassing
  P is TW(Boolean) {}
  Console.PutLine(P)

  R1:SomeThing[]
  R1.Append('A')

  R2:SomeThings
  R2.Append('A')
  
}

