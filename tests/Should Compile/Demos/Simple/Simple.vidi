{
  Items:Integer[]
  Console.PutLine(Items) // (blank)

  Items:=[]
  Console.PutLine(Items=[])  // True

  Some(X:Integer):Float { 3.1416*X }
  Console.PutLine(2+Some(3))  // 11.4248

  a:Integer:=123

  b::=a*0.5

  Console.PutLine(b)  // 61.5

  c:Integer[]
  Console.PutLine(c)
  
  Test(Message:Text) {
    Console.PutLine(Message)
  }
  
  Test("Hello")  // Hello
  
  SomeNumber:Float { 3.1415 }

  Console.PutLine(SomeNumber) // 3.1415

  Console.PutLine(2+2)  // 4
  Console.PutLine(2+SomeNumber)  // 5.1415

  for num::=-(2+1) to -1 { 
     Console.PutLine(num) } // -3 -2 -1

  for a in 1..3 { Console.Put(a) }  // 123

  for num::=2 to 4  {Console.Put(num)} // 234

  Console.PutLine

  d:Text[]:=["a",'b']
  Console.Put(d) // ab
 
  e:Text[]:=d+['c']
  Console.Put(e) // abc

  f::=e+d
  Console.PutLine(f) // abcab

//  g:Text[]:=d+'c'   <-- should fail 
//  Console.PutLine(g)

}
