Sieve(Number:Integer) {

    Primes:Integer[Number+1]
   
    for i in 2..Number {
        Primes[i] := i
    }

console.putline(primes)

console.putline

    i::= 2

    while ((i*i) <= Number)
    {
        if (Primes[i] <> 0)
        {
            for j in 2..number-1
            {
console.put(j, ' ')
                if primes[i]*j > number
                   break
                else
                   primes[primes[i]*j] :=0
            }
        }

        i:=i+1
    }

console.putline('ending')

    for i in 2..Number
    {
        if primes[i] <> 0
           Console.Put(primes[i],' ')
    }
}

{
  Sieve(100)
}
