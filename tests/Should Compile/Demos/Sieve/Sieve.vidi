Sieve(Number:Integer):Integer[] {

    Primes:Integer[Number+1]
   
    for i in 2..Number
        Primes[i] := i

    i::= 2

    while ((i*i) <= Number)
    {
        if (Primes[i] <> 0)
        {
            for j in 2..number-1
            {
                if primes[i]*j > number
                   break
                else
                   primes[primes[i]*j] :=0
            }
        }

        i += 1
    }
    
    return Primes
}

{
  Primes::=Sieve(100)
  
  for i in 2..100
      if Primes[i] <> 0
         Console.Put(Primes[i],' ')
}
